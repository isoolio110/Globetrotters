<script type="template" id="other-users-list-template">

  <p class="profile-section-header"><span class="glyphicon glyphicon-user" aria-hidden="true"></span><span class="glyphicon glyphicon-user" aria-hidden="true"></span> Other Globetrotters</p>
    <p>Read Stories Written by Other Globetrotters about the Places You Want to Travel to</p>
    <div id="other-users-icons-list">
    <%% otherUsers.each(function(user){ %>
      <div class="other-user-main-div">
        <div class="other-user-img">
          <img src=<%%= user.get('image_url') %> class="other-user-img">
        </div>
        <div class="other-user-shaded-div">
        </div>
        <div class="other-user-username-div">
          <a href="#profiles/<%%= user.get('id')%>"> <%%= user.get('username') %></a>
        </div>
      </div>
    <%% }) %>
  </div>
</script>