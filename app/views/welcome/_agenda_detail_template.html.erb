<script type="template" id="agenda-detail-template">
  <div class="header">
    <button class="done btn back-to-profile-btn">Back to Profile</button>
  </div>
  <div class="edit-form">
    <fieldset class="edit-form-field">
      <label> Location</label>
      <input id="location-location" type="string" name="place" value="<%%= place %>">
    </fieldset>
    <fieldset class="edit-form-field">
      <label>Planned Date</label>
      <input id="location-planned-date" type="string" name="planned_date" value="<%%= planned_date %>">
    </fieldset>
    <fieldset class="edit-form-field">
      <label>Visited Date</label>
      <input id="location-visited-date" type="string" name="visited_date" value="<%%= visited_date %>">
    </fieldset>
    <fieldset>
      <button class="btn btn-default save-btn">Save</button>
      <button class="btn btn-default delete-btn">Delete</button>
    </fieldset>
  </div>
</script>
